    <face_count_custom>0</face_count_custom>
    <reuse_depth>true</reuse_depth>
  </job>
  <job name="GenerateMasks" target="all">
    <masking_mode>3</masking_mode>
    <path/>
    <tolerance>0</tolerance>
  </job>
</batchjobs>
